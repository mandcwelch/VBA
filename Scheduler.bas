Sub color()

Dim rCell As Range
Dim rng As Range

Set rng = Range("a1", "q20")

For Each rCell In rng

    If rCell.Value = 1 Then

    rCell.Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.799981688894314
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.799981689
    End With
    
    ElseIf rCell.Value = 0 Then

    rCell.Select
  With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .TintAndShade = 0
    End With
    
End If

Next rCell

End Sub
