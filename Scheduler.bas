Sub Airport_Info()

Dim airport As String
Dim info As String
Dim message As String
Dim tobecopied As String



airport = InputBox("Please enter Aiport IATA")

If airport = "DFW" Then

message = "Driver will postion at baggage claim cooresponding to flight"

End If

If airport = "DAL" Then

message = "Greeter will postion at baggage claim cooresponding to flight"

End If

If airport = "ATL" Then

message = "You will never find your driver.  Give up and go home!"

End If

    
tobecopied = InputBox(airport & " Instructions", , message)


